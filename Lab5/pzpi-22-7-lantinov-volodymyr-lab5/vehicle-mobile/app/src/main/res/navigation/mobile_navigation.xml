<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_vehicles">

    <fragment
        android:id="@+id/navigation_vehicles"
        android:name="com.vehiclemonitor.app.ui.vehicles.VehiclesFragment"
        android:label="@string/my_vehicles"
        tools:layout="@layout/fragment_vehicles" />

    <fragment
        android:id="@+id/navigation_alerts"
        android:name="com.vehiclemonitor.app.ui.alerts.AlertsFragment"
        android:label="@string/alerts"
        tools:layout="@layout/fragment_alerts" />

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.vehiclemonitor.app.ui.profile.ProfileFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_profile" />

</navigation>